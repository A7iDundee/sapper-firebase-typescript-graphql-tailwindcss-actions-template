{
	"name": "sapper-firebase-typescript-graphql-tailwindcss-actions-template",
	"description": "A template that includes Svelte with Sapper, Firebase functions and hosting, TypeScript and TypeGraphQL, Tailwind CSS, ESLint and EditorConfig, and automatic building and deployment with GitHub Actions",
	"repository": {
		"type": "git",
		"url": "https://github.com/babichjacob/sapper-firebase-typescript-graphql-tailwindcss-actions-template.git"
	},
	"license": "MIT",
	"version": "2020.03.24",
	"scripts": {
		"firebase:deploy": "node ./node_modules/.bin/firebase deploy",
		"firebase:serve": "./node_modules/.bin/firebase serve",
		"sapper:dev": "node ./node_modules/sapper/dist/cli.js dev",
		"sapper:clean": "rimraf - functions/__sapper__/build",
		"sapper:setup": "mkdirp functions/__sapper__/build",
		"sapper:build": "node ./node_modules/.bin/cross-env NODE_ENV=production node ./node_modules/sapper/dist/cli.js build",
		"sapper:copy": "ncp ./__sapper__/build ./functions/__sapper__/build",
		"sapper:prod": "run-s sapper:clean sapper:setup sapper:build sapper:copy",
		"sapper:export": "node ./node_modules/sapper/dist/cli.js export",
		"dev": "npm run sapper:dev",
		"prod": "npm run sapper:prod",
		"deploy": "run-s prod firebase:deploy"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"express": "^4.17.1",
		"node-fetch": "^2.6.0",
		"sirv": "^0.4.0"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@babel/runtime": "^7.9.2",
		"@fullhuman/postcss-purgecss": "^2.1.0",
		"@pyoner/svelte-ts-preprocess": "^1.2.1",
		"@pyoner/svelte-types": "^3.4.4-2",
		"@rollup/plugin-commonjs": "^11.0.2",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^7.1.1",
		"@rollup/plugin-replace": "^2.3.1",
		"@tailwindcss/ui": "^0.1.3",
		"@types/express": "^4.17.3",
		"@types/node-fetch": "^2.5.5",
		"@typescript-eslint/eslint-plugin": "^2.25.0",
		"@typescript-eslint/parser": "^2.25.0",
		"apollo-server-express": "^2.11.0",
		"bufferutil": "^4.0.1",
		"connect": "^3.7.0",
		"cssnano": "^4.1.10",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-svelte3": "^2.7.3",
		"firebase-tools": "^7.16.0",
		"graphql": "^14.6.0",
		"mkdirp": "^1.0.3",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^7.0.27",
		"postcss-cli": "^7.1.0",
		"postcss-load-config": "^2.1.0",
		"postcss-preset-env": "^6.7.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rollup": "^1.32.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-svelte": "^5.1.1",
		"rollup-plugin-terser": "^5.3.0",
		"rollup-plugin-typescript2": "^0.21.2",
		"sapper": "^0.27.11",
		"svelte": "^3.20.1",
		"svelte-preprocess": "^3.5.0",
		"tailwindcss": "^1.2.0",
		"tslib": "^1.11.1",
		"type-graphql": "^0.17.6",
		"typescript": "^3.8.3",
		"utf-8-validate": "^5.0.2"
	}
}
